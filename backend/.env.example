# Application Settings
PROJECT_NAME=OnMaum API
VERSION=1.0.0
DESCRIPTION=OnMaum - Anonymous counseling platform for teenagers

# Security
SECRET_KEY=your-secret-key-here-change-in-production
JWT_SECRET_KEY=your-jwt-secret-key-here-change-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440
REFRESH_TOKEN_EXPIRE_DAYS=7

# Database
DATABASE_URL=postgresql://onmaum_user:onmaum_password@localhost:5432/onmaum_db

# CORS
CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173"]

# File Upload
UPLOAD_DIR=uploads
MAX_FILE_SIZE=5242880
ALLOWED_EXTENSIONS=["jpg", "jpeg", "png", "webp"]

# OAuth (Naver)
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret
NAVER_REDIRECT_URI=http://localhost:3000/auth/naver/callback